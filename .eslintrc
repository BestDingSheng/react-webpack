{
    "extends":"eslint-config-airbnb",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true,
            "modules": true
        }
    },
    "env":{
        "browser":true,
        "node":true,
        "es6":true
    },
    "parser":"babel-eslint",
    "rules":{
        "strict":0,
        "valid-jsdoc":1,
        "react/jsx-uses-react":1,
        "react/jsx-uses-vars":1,
        "react/react-in-jsx-scope":1,
        "react/jsx-filename-extension":0
        
    },
    "plugins":[
        "react"
    ]
}